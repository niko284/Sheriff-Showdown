{
  "name": "Sheriff-Showdown",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/shared",
      "$ignoreUnknownInstances": true,
      "packages": {
        "$path": "Packages",
        "$ignoreUnknownInstances": true
      },
      "vendor": {
        "$path": "vendor", 
        "$ignoreUnknownInstances": true
      },
      "comm": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
      "$path": "src/server",
      "ServerPackages": {
        "$path": "ServerPackages"
      },
      "nodeModules": {
        "$path": "node_modules"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      },
      "StarterCharacterScripts": {
        "$className": "StarterCharacterScripts",
        "$path": "src/starterCharacter"
      }
    }
  }
}